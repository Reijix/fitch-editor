packages:
  ./*.cabal
  
package *
  documentation: true

constraints:
  zlib +bundled-c-zlib

with-compiler:
  wasm32-wasi-ghc

with-hc-pkg:
  wasm32-wasi-ghc-pkg

source-repository-package
  type: git
  location: https://github.com/dmjio/miso
  branch: master

program-options
  ghc-options: -haddock